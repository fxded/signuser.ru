var http = require('http');

http.createServer(function(req, res) {
    console.log('Server Listen on 3000....');   
    res.writeHead(200);
    req.on('data', function(data){
        console.log('requset: ', data.toString());   
        res.write('Write: '+ data.toString());
    });
    req.on('end', function(){
        console.log('the end ');
        res.end();
    });
 }).listen(3002);
